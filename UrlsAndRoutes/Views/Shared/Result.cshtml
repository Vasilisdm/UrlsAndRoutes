@model Result
@{ Layout = null; }

<!DOCTYPE html>
<html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>Routing</title>
        <link rel="stylesheet" asp-href-include="lib/bootstrap/dist/css/*.min.css" />
    </head>
    <body>
        <table class="table table-bordered table-striped table-sm">
            <tr><th>Controller:</th><td>@Model.Controller</td></tr>
            <tr><th>Action:</th><td>@Model.Action</td></tr>
            @foreach (string key in Model.Data.Keys) {
                <tr><th>@key :</th><td>@Model.Data[key]</td></tr>
            }
        </table>
        <a asp-controller="Home" asp-action="CustomVariable" asp-route-id="Tue" asp-protocol="https"
        asp-host="myserver.domain.com" asp-fragment="fragment">This targets another controller</a>
        <a asp-route="Outbound"></a>
    </body>
</html>
